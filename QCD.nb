        RowBox[{"GammaU", "[", 
         RowBox[{"x", "-", "1"}], "]"}]}]}], "}"}], ",", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"h", "==", 
        RowBox[{"-", "1"}]}], ",", 
       RowBox[{
        FractionBox[
         RowBox[{"-", "1"}], 
         SqrtBox["2"]], "*", 
        FractionBox[
         FormBox[
          FormBox[
           FormBox[
            FormBox[
