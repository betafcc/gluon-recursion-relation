Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "*", " ", "This", " ", "is", " ", "a", " ", "helper", " ", "to", " ", 
    "get", " ", "a", " ", "non", " ", "linear", " ", "dependent", " ", 
    "vector", " ", "to", " ", "use", " ", "as", " ", "\"\<q\>\""}], " ", 
   "**)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"GenerateQ", "[", "v_", "]"}], ":=", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"IsLinearDependent", "[", 
        RowBox[{"v", ",", 
         RowBox[{"{", 
          RowBox[{"1", ",", "1", ",", "0", ",", "0"}], "}"}]}], "]"}], "===", 
       "True"}], ",", 
      RowBox[{"{", 
       RowBox[{"1", ",", "0", ",", "1", ",", "0"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"1", ",", "1", ",", "0", ",", "0"}], "}"}]}], "]"}]}], "\n", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"IsLinearDependent", "[", 
     RowBox[{"u_List", ",", "v_List"}], "]"}], ":=", 
    RowBox[{
     RowBox[{"Abs", "[", 
      RowBox[{
       RowBox[{"Abs", "[", 
        RowBox[{"u", ".", "v"}], "]"}], "-", 
       RowBox[{
        RowBox[{"Norm", "[", "u", "]"}], "*", 
        RowBox[{"Norm", "[", "v", "]"}]}]}], "]"}], "<", 
     RowBox[{"10", "^", 
      RowBox[{"-", "6"}]}]}]}]}]}]], "Input",
