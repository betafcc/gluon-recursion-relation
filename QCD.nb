Cell[BoxData[
 RowBox[{
  RowBox[{"SB", "[", 
   RowBox[{"x_", ",", "ls_", ",", "rs_"}], "]"}], ":=", " ", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
        RowBox[{
        "*", " ", "\"\<x\>\"", " ", "subscript", " ", "needs", " ", "to", " ",
          "be", " ", "prepended", " ", "to", " ", "the", " ", "list", " ", 
         "of", " ", "helicity"}], "-", 
        RowBox[{"vector", " ", "tuples"}]}], ",", " ", 
       RowBox[{
       "to", " ", "be", " ", "passed", " ", "as", " ", "arguments", " ", "on",
         " ", "calls", " ", "for", " ", "\"\<J\>\""}]}], " ", "**)"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"xls", "=", 
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{"{", "x", "}"}], ",", " ", "ls"}], "]"}]}], ",", " ", 
      RowBox[{"xrs", "=", 
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{"{", "x", "}"}], ",", " ", "rs"}], "]"}]}]}], 
     "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"2", 
      RowBox[{"MP", "[", 
       RowBox[{
        RowBox[{"\[Kappa]", "[", "rs", "]"}], ",", " ", 
        RowBox[{"J", "@@", "xls"}]}], "]"}], "*", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"J", "@@", "xrs"}], ")"}], "[", 
       RowBox[{"[", "x", "]"}], "]"}]}], "\[IndentingNewLine]", "-", 
     RowBox[{"2", 
      RowBox[{"MP", "[", 
       RowBox[{
        RowBox[{"\[Kappa]", "[", "ls", "]"}], ",", 
        RowBox[{"J", "@@", "xrs"}]}], "]"}], "*", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"J", "@@", "xls"}], ")"}], "[", 
       RowBox[{"[", "x", "]"}], "]"}]}], "\[IndentingNewLine]", "+", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"\[Kappa]", "[", "ls", "]"}], "-", 
